generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       String   @id @default(cuid())
  email    String   @unique
  name     String?
  password String
  habits   Habits[]
}

model Habits {
  id     String  @id @default(cuid())
  name   String
  status Boolean
  User   User?   @relation(fields: [userId], references: [id])
  userId String?

  @@index([userId])
}
